<!-- Page wrapper  -->
<!-- ============================================================== -->
<div class="page-wrapper">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <div class="page-breadcrumb">
        <div class="row">
            <div class="col-12 d-flex no-block align-items-center">
                <h4 class="page-title">Dashboard</h4>
                <div class="ms-auto text-end">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">
                                Library
                            </li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- End Bread crumb and right sidebar toggle -->


    <!-- Container fluid  -->
    <!-- ============================================================== -->
    <div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Sales Cards  -->
        <!-- ============================================================== -->
        <div class="row">
            <!-- Column -->
            <div class="col-md-6 col-lg-4 col-xlg-3">
                <div class="card card-hover">
                    <div class="box bg-warning text-center">
                        <div class="row">
                            <div class="col-6">
                                <h1 class="font-light text-white">
                                    <i class="mdi mdi-account-multiple"></i>
                                </h1>
                                <h6 class="text-white">Total Students</h6>
                            </div>
                            <div class="col-6 text-center" style="display:flex; justify-content: center; align-items: center;">
                                <h1 class="text-white"><%=students%></h1>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 col-xlg-3">
                <div class="card card-hover">
                    <div class="box bg-info text-center">
                        <div class="row">
                            <div class="col-5">
                                <h1 class="font-light text-white">
                                    <i class="mdi mdi-anchor"></i>
                                </h1>
                                <h6 class="text-white">PAYEE Tax</h6>
                            </div>
                            <div class="col-7 text-center" style="display:flex; justify-content: center; align-items: center;">
                                <h3 class="text-white">K <%=payee%></h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-4 col-xlg-3">
                <div class="card card-hover">
                    <div class="box bg-dark text-center">
                        <div class="row">
                            <div class="col-5">
                                <h1 class="font-light text-white">
                                    <i class="fas fa-fax"></i>
                                </h1>
                                <h6 class="text-white">Fees Balances</h6>
                            </div>
                            <div class="col-7 text-center" style="display:flex; justify-content: center; align-items: center;">
                                <h3 class="text-white">MK <%=totalBalances%></h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6 col-lg-4 col-xlg-3">
                <div class="card card-hover">
                    <div class="box bg-primary text-center">
                        <div class="row">
                            <div class="col-6">
                                <h1 class="font-light text-white">
                                    <i class="mdi mdi-readability"></i>
                                </h1>
                                <h6 class="text-white">Assets Recorded</h6>
                            </div>
                            <div class="col-6 text-center" style="display:flex; justify-content: center; align-items: center;">
                                <h1 class="text-white"><%=assets%></h1>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-4 col-xlg-3">
                <div class="card card-hover">
                    <div class="box bg-danger text-center">
                        <div class="row">
                            <div class="col-5">
                                <h1 class="font-light text-white">
                                    <i class="mdi mdi-arrow-top-right"></i>
                                </h1>
                                <h6 class="text-white">Expenditures</h6>
                            </div>
                            <div class="col-7 text-center" style="display:flex; justify-content: center; align-items: center;">
                                <h3 class="text-white">K <%=exp%></h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-4 col-xlg-3">
                <div class="card card-hover">
                    <div class="box bg-success text-center">
                        <div class="row">
                            <div class="col-5">
                                <h1 class="font-light text-white">
                                    <i class="mdi mdi-arrow-bottom-left"></i>
                                </h1>
                                <h6 class="text-white">Earnings</h6>
                            </div>
                            <div class="col-7 text-center" style="display:flex; justify-content: center; align-items: center;">
                                <h3 class="text-white">K <%=earns%></h>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Sales chart -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <div class="d-md-flex align-items-center">
                            <div>
                                <h4 class="card-title">Financial Analysis</h4>
                                <h5 class="card-subtitle">Overview of financial trasnactions.</h5>
                            </div>
                        </div>
                        <div class="row">
                            <!-- column -->
                            <div class="col-lg-12">
                                <div class="flot-chart">
                                    <canvas id="myAreaChart"></canvas>
                                </div>
                            </div>
                            <!-- column -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Sales chart -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Recent comment and chats -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title text-center">Outstanding Balances</h3>

                        <div class="col-12">
                            <div class="table-responsive">
                                <table id="zero_config" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Firstname</th>
                                            <th>Surname</th>
                                            <th>Class</th>
                                            <th>Phone</th>
                                            <th>Email</th>
                                            <th>Balance</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% balances.forEach(b => {%>
                                            <tr>
                                                <td>
                                                    <%=b.id%>
                                                </td>
                                                <td>
                                                    <%=b.firstname%>
                                                </td>
                                                <td>
                                                    <%=b.lastname%>
                                                </td>
                                                <td>
                                                    <%=b.class%>
                                                </td>
                                                <td>
                                                    <%=b.phone%>
                                                </td>
                                                <td>
                                                    <%=b.email%>
                                                </td>
                                                <td>
                                                    <p class="text-danger">
                                                        <%=b.balance%>
                                                    </p>
                                                </td>
                                            </tr>
                                        <% }); %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
            <!-- column -->
        </div>
        <!-- ============================================================== -->
        <!-- Recent comment and chats -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Container fluid  -->


    <!-- footer -->
    <!-- ============================================================== -->
    <footer class="footer text-center">
        All Rights Reserved by Matrix-admin. Designed and Developed by
        <a href="https://www.wrappixel.com">WrapPixel</a>.
    </footer>
    <!-- ============================================================== -->
    <!-- End footer -->
    <!-- ============================================================== -->
</div>
<!-- ============================================================== -->
<!-- End Page wrapper  -->